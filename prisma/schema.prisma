model FinanceAp {
  ap_id            Int     @id @default(autoincrement())
  ap_code          String? @db.VarChar(50)
  substantive_test String?
  objective        String?
  procedures       String?
  method           String?
  description      String?
  application      String?
  finance_risk_id  Int
  finance          finance @relation(fields: [finance_risk_id], references: [risk_id], onDelete: Cascade)

  @@map("finance_ap")
}

