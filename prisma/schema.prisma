generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model notes {
  id          Int       @id @default(autoincrement())
  title       String?   @db.VarChar(255)
  description String?
  created_at  DateTime? @default(now()) @db.Timestamp(6)
}
